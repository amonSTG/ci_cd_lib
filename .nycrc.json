{
    "check-coverage": true,
    "extension": [
        ".ts"
    ],
    "extends": "@istanbuljs/nyc-config-typescript",
    "exclude": [
        "**/*.d.ts"
    ],
    "include": [
        "src/**/*.ts"
    ],
    "reporter": [
        "lcov"
    ],
    "temp-dir": "./.tmp",
    "watermarks": {
        "lines": [
            80,
            95
        ],
        "functions": [
            80,
            95
        ],
        "branches": [
            80,
            95
        ],
        "statements": [
            80,
            95
        ]
    }
}